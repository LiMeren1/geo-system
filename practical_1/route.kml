    <Style id="route_line">
      <LineStyle><color>ff0000ff</color><width>4</width></LineStyle>
    </Style>

    <Placemark>
      <name>Золоті ворота</name>
      <description>Головна брама стародавнього Києва.</description>
      <Point><coordinates>30.5133,50.4488,0</coordinates></Point>
    </Placemark>

    <Placemark>
